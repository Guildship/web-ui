{
  "name": "guildship",
  "version": "1.0.0",
  "staticPath": "public",
  "scripts": {
    "build:fresh": "yarn clean && yarn build",
    "build": "yarn parcel build src/index.html --public-url / --detailed-report --experimental-scope-hoisting",
    "start": "yarn parcel src/index.html --public-url / --open --port 3000",
    "bsb:clean": "yarn bsb -clean-world",
    "bsb:make": "yarn bsb -make-world",
    "test": "echo \"lol\" && exit 1",
    "clean": "rimraf dist lib/* .cache .merlin",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "license": "EUPL-1.2",
  "dependencies": {
    "apollo-client": "^2.6.3",
    "bs-css": "^8.0.4",
    "polished": "^3.4.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "reason-apollo": "0.16.4",
    "reason-react": ">= 0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@glennsl/bs-jest": "^0.4.8",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-loader": "^8.0.6",
    "bs-platform": "5.0.4",
    "bsb-js": "^1.1.7",
    "graphql_ppx": "^0.2.8",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.1.2",
    "rimraf": "^2.6.3"
  }
}
