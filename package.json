{
  "name": "guildship",
  "version": "1.0.0",
  "staticPath": "public",
  "scripts": {
    "build:fresh": "yarn clean && yarn build",
    "build": "yarn parcel build public/index.html --public-url / --detailed-report --experimental-scope-hoisting",
    "start": "yarn parcel public/index.html --public-url / --open --port 3000",
    "bsb:clean": "yarn bsb -clean-world",
    "bsb:make": "yarn bsb -make-world",
    "test": "echo \"lol\" && exit 1",
    "clean": "rimraf dist lib/* .cache .merlin"
  },
  "license": "EUPL-1.2",
  "dependencies": {
    "bs-css": "^8.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reason-react": ">= 0.7.0"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.4.8",
    "bs-platform": "5.0.4",
    "bsb-js": "^1.1.7",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.1.2",
    "rimraf": "^2.6.3"
  }
}
