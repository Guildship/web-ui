{
  "name": "guildship",
  "version": "1.0.0",
  "staticPath": "public",
  "scripts": {
    "build:fresh": "yarn clean && yarn build",
    "build": "yarn bsb:make && yarn parcel build src/index.html --public-url / --detailed-report --experimental-scope-hoisting",
    "start": "yarn parcel src/index.html --public-url / --open --port 3000",
    "bsb:clean": "yarn bsb -clean-world",
    "bsb:make": "yarn bsb -make-world",
    "test": "echo \"lol\" && exit 1",
    "clean": "rimraf dist lib/* .cache .merlin",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "license": "EUPL-1.2",
  "dependencies": {
    "bs-css": "^9.0.1",
    "bs-dynamic-import": "^0.4.0",
    "bs-fetch": "^0.5.0",
    "bs-webapi": "^0.15.2",
    "polished": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-responsive": "8.0.0-beta.1",
    "reason-graphql-hooks": "^0.2.1",
    "reason-loadable": "^0.6.2",
    "reason-react": ">= 0.7.0",
    "restorative": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@glennsl/bs-jest": "^0.4.8",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addon-notes": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "babel-loader": "^8.0.6",
    "bs-platform": "5.0.6",
    "bs-storybook": "^0.3.0",
    "bsb-js": "^1.1.7",
    "graphql_ppx": "^0.2.8",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.1.2",
    "rimraf": "^2.6.3"
  }
}
